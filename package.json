{
  "name": "agentic-coding-dev-setup",
  "version": "1.0.0",
  "description": "Setup script for MCP servers in VS Code and VS Code Insiders",
  "private": true,
  "type": "module",
  "scripts": {
    "setup-mcp": "node setup-mcp-servers.mjs",
    "setup-mcp:dry-run": "node setup-mcp-servers.mjs --dry-run",
    "setup-mcp:quick": "node setup-mcp-servers.mjs --skip-neo4j --skip-neo4j-docker --skip-cache-warm"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
